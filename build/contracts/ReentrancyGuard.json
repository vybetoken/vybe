  "source": "// SPDX-License-Identifier: MIT\npragma solidity ^0.6.0;\n\nabstract contract ReentrancyGuard {\n    bool private _entered;\n\n    modifier noReentrancy() {\n        require(!_entered);\n        _entered = true;\n        _;\n        _entered = false;\n    }\n}\n",
